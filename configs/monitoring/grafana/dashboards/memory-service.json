{
  "dashboard": {
    "id": null,
    "title": "Memory Service Dashboard",
    "tags": ["memory", "llm"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Memory Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"memory-service\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "HTTP Requests Total",
        "type": "graph",
        "targets": [
          {
            "expr": "http_server_requests_seconds_count",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Response Time",
        "type": "graph",
        "targets": [
          {
            "expr": "http_server_requests_seconds_sum / http_server_requests_seconds_count",
            "refId": "A"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
